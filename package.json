    "lint-staged": "^15.2.1",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "semantic-release": "^23.0.0",
    "storybook": "^7.6.12",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "packageManager": "yarn@4.1.0"
}
